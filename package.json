{
  "name": "bilis-2",
  "version": "1.0.0",
  "repository": "git@github.com:Prodeko/Bilis-2.git",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:migrate:up": "yarn ts-node ./server/utils/migrator up",
    "dev:migrate:create": "yarn ts-node ./server/utils/migrator create --folder ./server/migrations --name new-migration.ts",
    "dev:migrate:down": "yarn ts-node ./server/utils/migrator down",
    "dev:migrate:pending": "yarn ts-node ./server/utils/migrator pending",
    "dev:migrate:executed": "yarn ts-node ./server/utils/migrator executed",
    "dev:create-test-data": "yarn ts-node -r tsconfig-paths/register ./server/utils/createTestData",
    "dev:start": "NODE_ENV=development next start",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "yarn jest",
    "cypress": "cypress run",
    "build-and-test": "yarn build && yarn start-server-and-test start 3000 'cypress'",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "lint:force": "biome lint --apply-unsafe .",
    "prettify": "prettier . --check",
    "prettify:fix": "prettier . --write",
    "cleancode:check": "yarn lint & yarn prettify",
    "cleancode:fix": "yarn lint:fix & yarn prettify:fix"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@headlessui/react": "^1.4.3",
    "@tanstack/react-table": "^8.7.9",
    "apexcharts": "^3.33.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.0.1",
    "emoji-picker-react": "^4.3.0",
    "lodash": "^4.17.21",
    "next": "14.1.4",
    "pg": "^8.7.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^18.2.0",
    "react-gauge-chart": "^0.4.0",
    "react-icons": "^4.3.1",
    "sass": "^1.54.0",
    "sequelize": "^6.15.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^2.2.2",
    "umzug": "3.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.2",
    "@next/eslint-plugin-next": "13.4.8",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/lodash": "4.14.178",
    "@types/node": "20.0.0",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/react-gauge-chart": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "autoprefixer": "^10.4.19",
    "cypress": "9.3.1",
    "eslint": "8.29.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "jest": "27.4.7",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.9.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "10.4.0",
    "tsconfig-paths": "4.0.0",
    "tsdoc": "0.0.4",
    "typescript": "5.1.3"
  }
}
