{
  "name": "bilis-2",
  "version": "1.0.0",
  "repository": "git@github.com:Prodeko/Bilis-2.git",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:migrate:up": "yarn ts-node ./server/utils/migrator up",
    "dev:migrate:create": "yarn ts-node ./server/utils/migrator create --folder ./server/migrations --name new-migration.ts",
    "dev:migrate:down": "yarn ts-node ./server/utils/migrator down",
    "dev:migrate:pending": "yarn ts-node ./server/utils/migrator pending",
    "dev:migrate:executed": "yarn ts-node ./server/utils/migrator executed",
    "dev:create-test-data": "yarn ts-node -r tsconfig-paths/register ./server/utils/createTestData",
    "dev:start": "NODE_ENV=development next start",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "test": "yarn jest",
    "cypress": "cypress run",
    "build-and-test": "yarn build && yarn start-server-and-test start 3000 'cypress'",
    "stylify": "stylelint **/*.scss",
    "prettify": "prettier --write './**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint ."
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.5",
    "@headlessui/react": "^1.4.3",
    "apexcharts": "^3.33.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "emoji-picker-react": "^4.3.0",
    "lodash": "^4.17.21",
    "next": "^13.1",
    "pg": "^8.7.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^18.2.0",
    "react-gauge-chart": "^0.4.0",
    "react-icons": "^4.3.1",
    "sass": "^1.54.0",
    "sequelize": "^6.15.0",
    "umzug": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "@types/react-dom": "^18.0.9",
    "@types/react-gauge-chart": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^12.5.1",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^27.4.7",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "tsdoc": "^0.0.4",
    "typescript": "^4.5.5",
    "umzug": "^3.0.0"
  }
}
